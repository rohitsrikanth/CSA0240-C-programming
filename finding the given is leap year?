#include<stdio.h>
#include<conio.h>
void main(){
int d,m,y;
clrscr();
printf("enter date:");
scanf("%d",&d);
printf("enter month:");
scanf("%d",&m);
printf("enter year:");
scanf("%d",&y);
if(y%4==0&&y%400==0&&y%100!=0){
printf("%d/%d/%d it is leap year",d,m,y);
}
else{
printf("%d/%d/%d it is not leap year",d,m,y);
}
getch();
}

